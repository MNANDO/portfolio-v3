# Build-time S3 credentials (private, used by content loader)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
S3_BLOG_BUCKET=

# Client-side S3 config (PUBLIC_, exposed to browser for admin page)
PUBLIC_AWS_REGION=us-east-1
PUBLIC_S3_ASSETS_BUCKET=
PUBLIC_COGNITO_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Cognito (PUBLIC_, exposed to browser for auth)
PUBLIC_COGNITO_AUTHORITY=https://cognito-idp.us-east-1.amazonaws.com/us-east-1_xxxxxxxxx
PUBLIC_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
PUBLIC_COGNITO_REDIRECT_URI=http://localhost:4321/admin
PUBLIC_COGNITO_HOSTED_DOMAIN=https://your-prefix.auth.us-east-1.amazoncognito.com
